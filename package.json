{
  "name": "digital_police_web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.19.8",
    "axios": "^0.19.0",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.21",
    "mobx": "^5.10.1",
    "mobx-react": "^6.1.1",
    "mobx-react-devtools": "^6.1.1",
    "mobx-react-router": "^4.0.7",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-loadable": "^5.5.0",
    "react-router-config": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-transition-group": "^4.1.1",
    "reflect-metadata": "^0.1.13",
    "tslib": "^1.10.0"
  },
  "scripts": {
    "build-css": "stylus -c src/styles/stylus/ --out src/styles/css",
    "watch-css": "yarn run build-css && stylus -c -w src/styles/stylus/ --out src/styles/css",
    "lint-styl": "stylint src/styles/stylus",
    "start-ts": "react-scripts-ts start",
    "start": "npm-run-all -p watch-css start-ts",
    "build-ts": "react-scripts-ts build",
    "build": "npm-run-all build-css build-ts",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@material-ui/core": "^4.1.2",
    "@types/jest": "^24.0.15",
    "@types/js-cookie": "^2.2.2",
    "@types/node": "^12.0.10",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@types/react-loadable": "^5.5.1",
    "@types/react-router": "^5.0.2",
    "@types/react-router-config": "^5.0.0",
    "@types/react-router-dom": "^4.3.4",
    "@types/react-transition-group": "^2.9.2",
    "@types/webpack-env": "^1.13.9",
    "husky": "^2.5.0",
    "npm-run-all": "^4.1.5",
    "react-scripts-ts": "3.1.0",
    "stylint": "^1.5.9",
    "stylus": "^0.54.5",
    "typescript": "^3.6.0-dev.20190626"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn lint-styl"
    }
  },
  "proxy": {
    "/api": {
      "target": "http://localhost:5000",
      "changeOrigin": true,
      "ws": false,
      "pathRewrite": {
        "^/api": "/api"
      }
    }
  }
}
